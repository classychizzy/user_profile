generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model address {
  addressId   Int          @id @default(autoincrement())
  state       String       @db.VarChar(255)
  city        String       @db.VarChar(255)
  street      String       @db.VarChar(255)
  profile     Int?         @unique(map: "profile")
  userprofile userprofile? @relation(fields: [profile], references: [profile], onDelete: NoAction, onUpdate: NoAction, map: "address_ibfk_1")
}

model user {
  first_name  String       @db.VarChar(255)
  last_name   String       @db.VarChar(255)
  password    String       @db.VarChar(50)
  email       String?      @unique(map: "email") @db.VarChar(255)
  created_at  DateTime?    @db.Date
  updated_at  DateTime?    @db.Date
  USERID      Int          @id @default(autoincrement())
  userprofile userprofile?
}

model userprofile {
  username       String   @db.VarChar(255)
  email          String   @db.VarChar(255)
  phone_number   String   @db.VarChar(25)
  profile        Int?     @unique(map: "profile")
  userprofile_Id Int      @id @default(autoincrement())
  address        address?
  user           user?    @relation(fields: [profile], references: [USERID], onDelete: NoAction, onUpdate: NoAction, map: "userprofile_ibfk_1")
}
